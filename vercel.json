{
  "version": 2,
  "builds": [
    {
      "src": "app/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/guest",
      "dest": "/index.js"
    },
    {
      "src": "/session",
      "dest": "/index.js"
    },
    {
      "src": "/users",
      "dest": "/index.js"
    },
    {
      "src": "/(.*\\.(js|css|ico|png|jpg|svg|json))",
      "dest": "/app/build/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/app/build/index.html"
    }
  ]
}